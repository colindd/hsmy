/* pages/contact/contact.wxss */
.container{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.logo{
  width:260rpx;
  height:260rpx;
}
.contact_btn{
  width:65%;
  height:80rpx;
  text-align:center;
  font-size:30rpx;
  line-height:80rpx;
  border-radius:10rpx;
  margin-top:30rpx;
}
.container text{
  font-size:30rpx;
  margin-top:16rpx;
}
.container text:first-child{
  margin-top:50rpx;
}
.take_phone{
  color:#fff;
  background:#FE657F;
  margin-top:80rpx;
}
.out_login{
  color:#888888;
  background:#eeeeee;
}
/* pages/register/register.wxss */
page{
  background-color: #fff;
}
.container{
  width:90%;
  margin:0 auto;
}
.login_main{
  width:100%;
  margin-top:30rpx;
}
.input_box{
  width:100%;
  border-bottom:1rpx solid #EEEEEE;
  margin-top:10rpx;
  height:100rpx;
  line-height:100rpx;
  overflow: hidden;
}
.clear_inp{
  width:10%;
}
.clear_inp image{
  width:50rpx;
  height:50rpx;
  margin:40rpx auto 0;
}
.label_left{
  width:10%;
}
.label_left image{
  width:50rpx;
  height:50rpx;
  margin:40rpx auto 0;
}
.label_mid{
  width:60%;
  margin-left:10rpx;
}
.label_mid input{
  height:100%;
  height:130rpx;
}
.code_right{
  color:#5592FA;
}
.label_right{
  width:70%;
  height:100%;
  margin-left:10rpx;
}
.label_right input{
  height:100%;
  height:130rpx;
}
.show_psd{
  width:10%;
}
.show_psd image{
  width:50rpx;
  height:50rpx;
  margin:40rpx auto 0;
}
.tip_box{
  height:100rpx;
  line-height:100rpx;
  margin-top:10rpx;
}
.tip_box text{
  color:#5592FA;
}

.reg_btn{
  width:100%;
  margin-top:200rpx;
  line-height:100rpx;
  border-radius:20rpx;
  font-size:32rpx;
  text-align:center;
  color:#fff;
  background: #F9547E;
  box-shadow: 0px 6px 12px rgba(249, 84, 126, 0.51);
}
.tip_text{
  margin-top:40rpx;
  text-align:center;
  color:#B2B2B2;
}
.tip_text text{
  color:#F9547E;
}
/* pages/uploadSuccess/uploadSuccess.wxss */
.container{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.suc_icon{
  width:120rpx;
  height:120rpx;
  display:block;
}
.tip_success{
  text-align:center;
  font-size:34rpx;
  font-weight:bold;
  color:#333;
  margin-top:50rpx;
}
.back_index{
  width:75%;
  margin:120rpx auto 0;
  border-radius:20rpx;
  height:90rpx;
  text-align:center;
  box-sizing: border-box;
  line-height:90rpx;
  border:1rpx solid #FE657F;
  color:#FE657F;
  font-size:32rpx;
}